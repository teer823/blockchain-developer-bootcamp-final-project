  "scripts": {
    "lint": "standard",
    "ganache": "export NODE_OPTIONS=--openssl-legacy-provider && npx ganache-cli --networkId 1337 --chainId 1337 --port 8545 -m 'census blame faint universe one code fit father cereal tomato prepare excuse door extend that race forest weekend dynamic ginger visual love wood saddle'",
