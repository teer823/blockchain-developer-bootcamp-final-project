  "standard": {
    "parser": "babel-eslint",
    "envs": [
      "mocha",
      "browser"
    ],
    "globals": [
      "artifacts",
      "contract",
      "assert"
    ]
  },
  "devDependencies": {
    "@opengsn/dev": "^2.2.4",
    "babel-eslint": "^10.1.0",
    "solc": "^0.8.10",
    "standard": "^16.0.4",
    "web3": "^1.6.0",
    "web3-utils": "^1.6.0"
  },
  "dependencies": {
    "@opengsn/contracts": "^2.2.4",
    "@openzeppelin/contracts": "^4.3.2",
    "dotenv": "^10.0.0",
    "ganache-cli": "^6.12.2",
    "truffle-hdwallet-provider": "^1.0.17"
  }
